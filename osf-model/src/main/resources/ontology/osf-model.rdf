    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://www.dataconservancy.org/osf-business-object-model#BusinessObject -->

    <owl:Class rdf:about="&osfbom;OSFBusinessObject">
        <rdfs:label xml:lang="en">OSFBusinessObject</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <!-- TODO: probably should be a string; OSF ids seem to be strings, 
                    five characters in length -->
                <owl:onProperty rdf:resource="&osfbom;hasId"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
                <owl:onProperty rdf:resource="&osfbom;hasType"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
          </owl:Restriction>
    </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">
        A Business Object is a common superclass for the OSF objects "User", "Collection", "NodeBase", "FileBase" and "Registration"
      </rdfs:comment>
    </owl:Class>
    
   <!-- http://www.dataconservancy.org/osf-business-object-model#User -->
   <owl:Class rdf:about="&osfbom;User">
        <rdfs:label xml:lang="en">User</rdfs:label>
        <rdfs:subClassOf rdf:resource="&osfbom;OSFBusinessObject"/>
        <owl:disjointWith rdf:resource="&osfbom;NodeBase"/>
        <owl:disjointWith rdf:resource="&osfbom;Collection"/>
    <owl:disjointWith rdf:resource="&osfbom;Contributor"/>
    <owl:disjointWith rdf:resource="&osfbom;FileBase"/>
        <rdfs:comment xml:lang="en">
      A User is ...
      </rdfs:comment>
   </owl:Class>

   <!-- http://www.dataconservancy.org/osf-business-object-model#NodeBase -->
   <owl:Class rdf:about="&osfbom;NodeBase">
     <rdfs:subClassOf rdf:resource="&osfbom;OSFBusinessObject"/>
        <owl:disjointWith rdf:resource="&osfbom;Collection"/>
    <owl:disjointWith rdf:resource="&osfbom;FileBase"/>
    <owl:disjointWith rdf:resource="&osfbom;Contributor"/>
    <owl:disjointWith rdf:resource="&osfbom;User"/>
    <rdfs:comment xml:lang="en">
      NodeBase is a common superclass for Node and Registration
      </rdfs:comment>
   </owl:Class>
   
    <!-- http://www.dataconservancy.org/osf-business-object-model#Node -->
   <owl:Class rdf:about="&osfbom;Node">
        <rdfs:label xml:lang="en">Node</rdfs:label>
        <rdfs:subClassOf rdf:resource="&osfbom;NodeBase"/>
        <rdfs:comment xml:lang="en">
      A Node is ...
      </rdfs:comment>
   </owl:Class>


   <!-- http://www.dataconservancy.org/osf-business-object-model#Registration -->
   <owl:Class rdf:about="&osfbom;Registration">
        <rdfs:label xml:lang="en">Registration</rdfs:label>
        <rdfs:subClassOf rdf:resource="&osfbom;NodeBase"/>
   </owl:Class>  


    <!-- http://www.dataconservancy.org/osf-business-object-model#Collection -->
   <owl:Class rdf:about="&osfbom;Collection">
        <rdfs:label xml:lang="en">Collection</rdfs:label>
        <rdfs:subClassOf rdf:resource="&osfbom;OSFBusinessObject"/>
        <owl:disjointWith rdf:resource="&osfbom;User"/>
        <owl:disjointWith rdf:resource="&osfbom;NodeBase"/>
    <owl:disjointWith rdf:resource="&osfbom;FileBase"/>
    <owl:disjointWith rdf:resource="&osfbom;Contributor"/>  
        <rdfs:comment xml:lang="en">
      A Node is ...
      </rdfs:comment>
   </owl:Class>

      
   <!-- http://www.dataconservancy.org/business-object-model#DataEntity -->
   <owl:Class rdf:about="&osfbom;DataEntity">
       <rdfs:subClassOf rdf:resource="&osfbom;OSFBusinessObject"/>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&osfbom;Collection"/>
                    <rdf:Description rdf:about="&osfbom;NodeBase"/>
            <rdf:Description rdf:about="&osfbom;File"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>     
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&osfbom;hasDateCreated"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&osfbom;hasDateModified"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    
    <!-- http://www.dataconservancy.org/osf-business-object-model#Contributor -->
    <!-- TODO: consider subclassing foaf:Person -->
    <owl:Class rdf:about="&osfbom;Contributor">
      <rdfs:label xml:lang="en">Contributor</rdfs:label>
      <rdfs:subClassOf rdf:resource="&osfbom;OSFBusinessObject"/>
    </owl:Class>

    <!-- http://www.dataconservancy.org/osf-business-object-model#Relationship -->
    <owl:Class rdf:about="&osfbom;Relationship">
      <rdfs:label xml:lang="en">Relationship</rdfs:label>
      <rdfs:disjointWith rdf:resource="&osfbom;OSFBusinessObject"/>
      <rdfs:disjointWith rdf:resource="&osfbom;LinkBase"/>
    </owl:Class>
    
    <!-- http://www.dataconservancy.org/osf-business-object-model#LinkBase -->
    <!-- TODO: difference/reasoning for Link and LinkBase? -->
    <owl:Class rdf:about="&osfbom;LinkBase">
      <rdfs:label xml:lang="en">LinkBase</rdfs:label>
      <owl:disjointWith rdf:resource="&osfbom;OSFBusinessObject"/>
      <owl:disjointWith rdf:resource="&osfbom;Relationship"/>
    </owl:Class>      
    
    <!-- http://www.dataconservancy.org/osf-business-object-model#Link -->
    <owl:Class rdf:about="&osfbom;Link">
      <rdfs:label xml:lang="en">Link</rdfs:label>
      <rdfs:subClassOf rdf:resource="&osfbom;LinkBase"/>
    </owl:Class>

    <!-- TODO: difference between Link and RelatedLink?  I think 
        they can be collapsed? -->
    <!-- "Link" describes the resource object, "relatedlink" describes a
        "relationship", I think -->
    <!-- TODO: consider creating a vocabulary from https://tools.ietf.org/html/rfc5988 to
        represent links and their properties (like relationship type, etc.). -->
    <!-- http://www.dataconservancy.org/osf-business-object-model#RelatedLink -->
    <owl:Class rdf:about="&osfbom;RelatedLink">
      <rdfs:label xml:lang="en">RelatedLink</rdfs:label>
      <rdfs:subClassOf rdf:resource="&osfbom;LinkBase"/>
    </owl:Class>

    <!-- http://www.dataconservancy.org/osf-business-object-model#FileBase -->
    <owl:Class rdf:about="&osfbom;FileBase">
      <rdfs:label xml:lang="en">FileBase</rdfs:label>
      <rdfs:subClassOf rdf:resource="&osfbom;OSFBusinessObject"/>
      <owl:disjointWith rdf:resource="&osfbom;User"/>
      <owl:disjointWith rdf:resource="&osfbom;NodeBase"/>
      <owl:disjointWith rdf:resource="&osfbom;Contributor"/>
      <owl:disjointWith rdf:resource="&osfbom;Collection"/>
    </owl:Class>    
    
    <!-- http://www.dataconservancy.org/osf-business-object-model#File --> 
    <owl:Class rdf:about="&osfbom;File">
      <rdfs:label xml:lang="en">File</rdfs:label>
      <rdfs:subClassOf rdf:resource="&osfbom;FileBase"/>
      <owl:disjointWith rdf:resource="&osfbom;Folder"/>
      <owl:disjointWith rdf:resource="&osfbom;Provider"/>
    </owl:Class>    

    <!-- http://www.dataconservancy.org/osf-business-object-model#Folder -->
    <owl:Class rdf:about="&osfbom;Folder">
      <rdfs:label xml:lang="en">Folder</rdfs:label>
      <rdfs:subClassOf rdf:resource="&osfbom;FileBase"/>
      <owl:disjointWith rdf:resource="&osfbom;File"/>
      <owl:disjointWith rdf:resource="&osfbom;Provider"/>
    </owl:Class>    

    <!-- http://www.dataconservancy.org/osf-business-object-model#Provider -->
    <owl:Class rdf:about="&osfbom;Provider">
      <rdfs:label xml:lang="en">Provider</rdfs:label>
      <rdfs:subClassOf rdf:resource="&osfbom;FileBase"/>
      <owl:disjointWith rdf:resource="&osfbom;File"/>
      <owl:disjointWith rdf:resource="&osfbom;Folder"/>
    </owl:Class>

    <!-- http://www.dataconservancy.org/osf-business-object-model#FileEntity -->
    <owl:Class rdf:about="&osfbom;FileEntity">
      <rdfs:label xml:lang="en">FileEntity</rdfs:label>
      <rdfs:subClassOf rdf:resource="&osfbom;OSFBusinessObject"/>
      <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&osfbom;File"/>
            <rdf:Description rdf:about="&osfbom;Folder"/>
                </owl:unionOf>
            </owl:Class>
       </owl:equivalentClass>  
    </owl:Class>
  
    <!-- TODO: Comments that may appear on a Project (or other node categories?) are
        not represented in this ontology -->
</rdf:RDF>
