    <!-- http://www.dataconservancy.org/osf-business-object-model#BusinessObject -->

    <owl:Class rdf:about="&osfbom;OSFBusinessObject">
        <rdfs:label xml:lang="en">OSFBusinessObject</rdfs:label>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&osfbom;hasId"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
    </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">
        A Business Object is a common superclass for the OSF objects "User", "Collection", "NodeBase", "FileBase" and "Registration"
      </rdfs:comment>
    </owl:Class>
    
    <!-- http://www.dataconservancy.org/osf-business-object-model#Log -->
    
    <owl:Class rdf:about="&osfbom;Log">
        <rdfs:label xml:lang="en">Log</rdfs:label>
        <rdfs:subClassOf rdf:resource="&osfbom;OSFBusinessObject"/>
        <owl:disjointWith rdf:resource="&osfbom;NodeBase"/>
        <owl:disjointWith rdf:resource="&osfbom;Collection"/>
        <owl:disjointWith rdf:resource="&osfbom;Contributor"/>
        <owl:disjointWith rdf:resource="&osfbom;FileBase"/>
        <owl:disjointWith rdf:resource="&osfbom;User"/>
        <rdfs:comment xml:lang="en">
            Encapsulates an action performed by an agent to accomplish some task
        </rdfs:comment>
    </owl:Class>
    
    <!-- http://www.dataconservancy.org/osf-business-object-model#License -->
    
    <!-- TODO: is this really a sub-class of OSFBusinessObject?  Liceneses really have 
        no identity outside of the content of a Node, so can they legitimatly claim to be
        OSF business objects? -->
    <owl:Class rdf:about="&osfbom;License">
        <rdfs:label xml:lang="en">License</rdfs:label>
        <rdfs:subClassOf rdf:resource="&osfbom;OSFBusinessObject"/>
        <rdfs:comment xml:lang="en">
            Represents a licence governing the use of the information encapsulated by a Node or Registration.
        </rdfs:comment>
    </owl:Class>
    
