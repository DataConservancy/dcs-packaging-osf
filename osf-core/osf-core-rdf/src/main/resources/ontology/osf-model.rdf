    <owl:Ontology rdf:about="http://www.dataconservancy.org/osf-business-object-model">
        <dcterms:modified rdf:datatype="&xsd;date">2015-08-18</dcterms:modified>
        <owl:versionInfo>2015/08/14</owl:versionInfo>
        <rdfs:comment xml:lang="en">Ontology for the Data Conservancy OSF Business Object Model</rdfs:comment>
        <dcterms:publisher rdf:resource="http://www.dataconservancy.org/"/>
    </owl:Ontology>
    



    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- http://www.dataconservancy.org/osf-business-object-model#hasFile -->

    <owl:ObjectProperty rdf:about="&osfbom;hasFile">
        <rdfs:label xml:lang="en">hasFile</rdfs:label>
        <rdfs:comment xml:lang="en">An object property specifying a file or directory belonging to a node</rdfs:comment>
        <rdfs:domain rdf:resource="&osfbom;Folder"/>
        <rdfs:range rdf:resource="&osfbom;FileBase"/>
    </owl:ObjectProperty>

    
    <!-- http://www.dataconservancy.org/osf-business-object-model#hasContributor -->
    

    <owl:ObjectProperty rdf:about="&osfbom;hasContributor">
        <rdfs:label xml:lang="en">hasContributor</rdfs:label>
        <rdfs:comment xml:lang="en">An object property specifying a contributor belonging to a node</rdfs:comment>
        <rdfs:domain rdf:resource="&osfbom;Node"/>
        <rdfs:range rdf:resource="&osfbom;Contributor"/>
    </owl:ObjectProperty>
    
    <!-- http://www.dataconservancy.org/osf-business-object-model#hasUser -->
    
    <owl:ObjectProperty rdf:about="&osfbom;hasUser">
        <rdfs:label xml:lang="en">hasUser</rdfs:label>
        <rdfs:comment xml:lang="en">An object property specifying a user belonging to a contributor</rdfs:comment>
        <rdfs:domain rdf:resource="&osfbom;Contributor"/>
        <rdfs:range rdf:resource="&osfbom;User"/>
    </owl:ObjectProperty>
    
    <!-- http://www.dataconservancy.org/osf-business-object-model#hasNode -->
    <owl:ObjectProperty rdf:about="&osfbom;hasNode">
        <rdfs:label xml:lang="en">hasNode</rdfs:label>
        <rdfs:comment xml:lang="en">An object property specifying an owning node for a provider or comment</rdfs:comment>
        <rdfs:domain rdf:resource="&osfbom;OSFBusinessObject"/>
        <rdfs:range rdf:resource="&osfbom;Node"/>
    </owl:ObjectProperty>    
    
    <owl:ObjectProperty rdf:about="&osfbom;registeredFrom">
        <rdfs:label xml:lang="en">registeredFrom</rdfs:label>
        <rdfs:comment xml:lang="en">An object property specifying the Node that the Registration was registered from.</rdfs:comment>
        <rdfs:domain rdf:resource="&osfbom;Registration"/>
        <rdfs:range rdf:resource="&osfbom;Node"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&osfbom;registeredBy">
        <rdfs:label xml:lang="en">registeredBy</rdfs:label>
        <rdfs:comment xml:lang="en">An object property specifying the User that created the Registration</rdfs:comment>
        <rdfs:domain rdf:resource="&osfbom;Registration"/>
        <rdfs:range rdf:resource="&osfbom;User"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&osfbom;hasRoot">
        <rdfs:label xml:lang="en">hasRoot</rdfs:label>
        <rdfs:comment xml:lang="en">An object property specifying root Node in a graph of Nodes.</rdfs:comment>
        <rdfs:domain rdf:resource="&osfbom;NodeBase"/>
        <rdfs:range rdf:resource="&osfbom;NodeBase"/>
        <rdf:type rdf:resource="&jsonapi;Relationship"/>
        <jsonapi:relName>root</jsonapi:relName>            
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&osfbom;hasParent">
        <rdfs:label xml:lang="en">hasParent</rdfs:label>
        <rdfs:comment xml:lang="en">An object property specifying the parent Node of a Node.</rdfs:comment>
        <rdfs:domain rdf:resource="&osfbom;NodeBase"/>
        <rdfs:range rdf:resource="&osfbom;NodeBase"/>
        <rdf:type rdf:resource="&jsonapi;Relationship"/>
        <jsonapi:relName>parent</jsonapi:relName>            
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&osfbom;hasChild">
        <rdfs:label xml:lang="en">hasChild</rdfs:label>
        <rdfs:comment xml:lang="en">An object property specifying the child of a Node.</rdfs:comment>
        <rdfs:domain rdf:resource="&osfbom;NodeBase"/>
        <rdfs:range rdf:resource="&osfbom;NodeBase"/>
        <rdf:type rdf:resource="&jsonapi;Relationship"/>
        <jsonapi:relName>children</jsonapi:relName>            
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&osfbom;hasLogAgent">
        <rdfs:label xml:lang="en">hasLogAgent</rdfs:label>
        <rdfs:comment xml:lang="en">The agent responsible for performing the action</rdfs:comment>
        <rdfs:domain rdf:resource="&osfbom;Log"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&osfbom;hasLogTarget">
        <rdfs:label xml:lang="en">hasLogTarget</rdfs:label>
        <rdfs:comment xml:lang="en">The OSF Node, User, ... that was the target of the action.</rdfs:comment>
        <rdfs:domain rdf:resource="&osfbom;Log"/>
        <rdfs:range rdf:resource="&osfbom;OSFBusinessObject"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&osfbom;hasLogSource">
        <rdfs:label xml:lang="en">hasLogSource</rdfs:label>
        <rdfs:comment xml:lang="en">The OSF Node, User, ... that was the source of the action.</rdfs:comment>
        <rdfs:domain rdf:resource="&osfbom;Log"/>
        <rdfs:range rdf:resource="&osfbom;OSFBusinessObject"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&osfbom;hasLicense">
        <rdfs:label xml:lang="en">hasLicense</rdfs:label>
        <rdfs:comment xml:lang="en">The license that governs the possible re-use of the content relating to a Node or Registration</rdfs:comment>
        <rdfs:domain rdf:resource="&osfbom;NodeBase"/>
        <rdfs:range rdf:resource="&osfbom;License"/>
    </owl:ObjectProperty>
    
    <!-- http://www.dataconservancy.org/osf-business-object-model#hasProvider -->
    
    <owl:ObjectProperty rdf:about="&osfbom;hasProvider">
        <rdfs:label xml:lang="en">hasProvider</rdfs:label>
        <rdfs:comment xml:lang="en">A property referencing a storage provider for a Node</rdfs:comment>
        <rdfs:domain rdf:resource="&osfbom;Node"/>
        <rdfs:range rdf:resource="&osfbom;Provider"/>
    </owl:ObjectProperty>
    
    <!-- http://www.dataconservancy.org/osf-business-object-model#providedBy -->
    
    <owl:ObjectProperty rdf:about="&osfbom;providedBy">
        <rdfs:label xml:lang="en">providedBy</rdfs:label>
        <rdfs:comment xml:lang="en">A property referencing the storage provider for a file or folder</rdfs:comment>
        <rdfs:domain rdf:resource="&osfbom;FileEntity"/>
        <rdfs:range rdf:resource="&osfbom;Provider"/>
    </owl:ObjectProperty>

    <!-- osfbom:Comment -->

    <owl:ObjectProperty rdf:about="&osfbom;authoredBy">
        <rdfs:label xml:lang="en">authoredBy</rdfs:label>
        <rdfs:comment xml:lang="en">The user who authored the comment or wiki page</rdfs:comment>
        <rdfs:domain rdf:resource="&osfbom;OSFBusinessObject"/>
        <rdfs:range rdf:resource="&osfbom;User"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&osfbom;hasWiki">
        <rdfs:label xml:lang="en">hasWiki</rdfs:label>
        <rdfs:comment xml:lang="en">A Wiki page that is attached to the node</rdfs:comment>
        <rdfs:domain rdf:resource="&osfbom;NodeBase"/>
        <rdfs:range rdf:resource="&osfbom;Wiki"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&osfbom;hasComment">
        <rdfs:label xml:lang="en">hasComment</rdfs:label>
        <rdfs:comment xml:lang="en">References the comment that is attached to the CommentTarget</rdfs:comment>
        <rdfs:domain rdf:resource="&osfbom;CommentTarget"/>
        <rdfs:range rdf:resource="&osfbom;Comment"/>
    </owl:ObjectProperty>

