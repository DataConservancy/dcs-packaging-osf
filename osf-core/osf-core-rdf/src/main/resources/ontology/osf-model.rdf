    <!-- http://www.dataconservancy.org/osf-business-object-model#Provider -->
    <owl:Class rdf:about="&osfbom;Provider">
      <rdfs:label xml:lang="en">Provider</rdfs:label>
      <rdfs:subClassOf rdf:resource="&osfbom;FileBase"/>
      <owl:disjointWith rdf:resource="&osfbom;File"/>
      <owl:disjointWith rdf:resource="&osfbom;Folder"/>
    </owl:Class>

    <!-- http://www.dataconservancy.org/osf-business-object-model#FileEntity -->
    <owl:Class rdf:about="&osfbom;FileEntity">
      <rdfs:label xml:lang="en">FileEntity</rdfs:label>
      <rdfs:subClassOf rdf:resource="&osfbom;OSFBusinessObject"/>
      <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&osfbom;File"/>
            <rdf:Description rdf:about="&osfbom;Folder"/>
                </owl:unionOf>
            </owl:Class>
       </owl:equivalentClass>  
    </owl:Class>

    <!-- http://www.dataconservancy.org/osf-business-object-model#Comment -->
    <owl:Class rdf:about="&osfbom;Comment">
      <rdfs:label xml:lang="en">Comment</rdfs:label>
      <rdfs:subClassOf rdf:resource="&osfbom;OSFBusinessObject"/>
      <rdfs:subClassOf rdf:resource="&osfbom;DataEntity"/>
    </owl:Class>

    <owl:Class rdf:about="&osfbom;CommentTarget">
      <rdfs:label xml:lang="en">CommentTarget</rdfs:label>
      <rdfs:comment xml:lang="en">Members of the CommentTarget class may have Comments placed on them (i.e. be the "target" of a Comment)</rdfs:comment>
      <rdfs:subClassOf>
        <owl:Class>
          <owl:unionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="&osfbom;Comment"/>
            <rdf:Description rdf:about="&osfbom;NodeBase"/>
            <rdf:Description rdf:about="&osfbom;Wiki"/>
          </owl:unionOf>
        </owl:Class>
      </rdfs:subClassOf>
    </owl:Class>
</rdf:RDF>
